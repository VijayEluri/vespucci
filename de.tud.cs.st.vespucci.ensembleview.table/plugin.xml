<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="de.tud.cs.st.vespucci.view"
            class="de.tud.cs.st.vespucci.view.ensemble_elements.views.EnsembleElementsTableView"
            icon="icons/Vespucci_modelDiagramFile.gif"
            id="de.tud.cs.st.vespucci.view.ensemble_elements"
            name="Ensemble Elements">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="de.tud.cs.st.vespucci.ensembleview.table.views.EnsembleElementsVisualizer"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="de.tud.cs.st.vespucci.diagram.resultProcessors">
      <resultProcessor
            ResultProcessor="de.tud.cs.st.vespucci.view.ensemble_elements.EnsembleElementsVisualizer">
      </resultProcessor>
   </extension>

</plugin>
