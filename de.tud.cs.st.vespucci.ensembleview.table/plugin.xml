<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="de.tud.cs.st.vespucci.ensembleview.table"
            name="Vespucci">
      </category>
      <view
            category="de.tud.cs.st.vespucci.ensembleview.table"
            class="de.tud.cs.st.vespucci.ensembleview.table.views.EnsembleElementsTableView"
            icon="icons/sample.gif"
            id="de.tud.cs.st.vespucci.ensembleview.table"
            name="Ensemble Elements Table">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="de.tud.cs.st.vespucci.ensembleview.table.views.EnsembleElementsVisualizer"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="de.tud.cs.st.vespucci.diagram.resultProcessors">
      <resultProcessor
            ResultProcessor="de.tud.cs.st.vespucci.ensembleview.table.EnsembleElementsVisualizer">
      </resultProcessor>
   </extension>

</plugin>
