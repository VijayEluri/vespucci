<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="generatePrologFacts"
         name="Generate Prolog Facts"
         point="de.tud.cs.st.vespucci.diagram.doSave">
      <storage
            Label="Generate/Save Prolog File"
            StorageClient="de.tud.cs.st.vespucci.generateprologfacts.GenerateProlog"
