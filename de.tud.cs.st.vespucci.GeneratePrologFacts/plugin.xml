<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="generatePrologFacts"
         name="name"
         point="de.tud.cs.st.vespucci.diagram.saveActions">
      <saveAction
            Label="Generate/Save Prolog File"
            SaveAction="de.tud.cs.st.vespucci.generateprologfacts.GenerateProlog"
            id="savePrologFile">
      </saveAction>
   </extension>
   <extension
         point="de.tud.cs.st.vespucci.diagram.diagramProcessors">
      <diagramProcessor
            DiagramProcessor="de.tud.cs.st.vespucci.generateprologfacts.GenerateProlog"
            Label="Generate Prolog Facts">
      </diagramProcessor>
   </extension>

</plugin>
